function createnewbox(num)
    print("num: "..num)
    if num > 1 then

            local oop = Instance.new("Highlight", doorss[num])
    local box = doorss[num]:Clone()
    box.Parent = doorss[num]
    box.Position += Vector3.new(-5,0,0)
    box.Name = "box"

    local oop = Instance.new("Highlight", box)
 	box.ProximityPrompt:Destroy()

    return end    
    local oop = Instance.new("Highlight", doorss[num])
    local box = doorss[num]:Clone()
    box.Parent = doorss[num]
    box.Position += Vector3.new(0,0,-5)
    box.Name = "box"
	box.ProximityPrompt:Destroy()
    local oop = Instance.new("Highlight", box)

end
-- doors[1].Highlight:Destroy()

for i,v in pairs(doorss) do
    print("i: "..i)
    createnewbox(i)
end

print("success!")

